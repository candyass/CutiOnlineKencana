<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:dist="http://schemas.android.com/apk/distribution"
          package="com.rs.kencana">

    <dist:module dist:instant="true"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
            android:name=".MyApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher_2"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_2_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".view.detailcuti.DetailCutiActivity">
        </activity>
        <activity
                android:name=".view.detailpegawai.DetailPegawaiActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".view.MainAdminActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".view.pengajuan.PengajuanCutiActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".view.daftar.DaftarActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".view.login.LoginActivity"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".view.MainActivity"
                android:label="@string/app_name"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".view.tandatangan.TandaTanganActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".view.tandatanganadmin.TandaTanganAdminActivity"
                android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>