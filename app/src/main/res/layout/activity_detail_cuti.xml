<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/detail_cuti_root"
        tools:context=".view.detailcuti.DetailCutiActivity">

    <TextView
            android:text="TextView"
            android:textColor="@color/colorPrimary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/ic_tanggal_24dp"
            android:layout_marginEnd="8dp"
            android:id="@+id/detail_cuti_list_pengajuan_cuti_tanggal_cuti_text" app:layout_constraintEnd_toEndOf="parent"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/detail_cuti_list_pengajuan_cuti_nama_text"/>
    <TextView
            android:text="Dari Tanggal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/detail_cuti_textView22"
            app:layout_constraintBaseline_toBaselineOf="@+id/detail_cuti_list_pengajuan_cuti_tanggal_cuti_text"
            app:layout_constraintEnd_toStartOf="@+id/detail_cuti_list_pengajuan_cuti_tanggal_cuti_text" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Menu"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:drawableLeft="@drawable/ic_tanggal_24dp"
            android:textColor="@color/colorPrimary"
            android:layout_height="wrap_content"
            android:id="@+id/detail_cuti_list_pengajuan_cuti_tanggal_akhir_cuti_text" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/detail_cuti_list_pengajuan_cuti_tanggal_cuti_text" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:text="Sampai Tanggal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/detail_cuti_textView24"
            app:layout_constraintBaseline_toBaselineOf="@+id/detail_cuti_list_pengajuan_cuti_tanggal_akhir_cuti_text"
            app:layout_constraintEnd_toStartOf="@+id/detail_cuti_list_pengajuan_cuti_tanggal_akhir_cuti_text" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Menu"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="8dp"
            android:textColor="@color/colorTextIcon"
            android:background="@drawable/round_title"
            android:id="@+id/detail_cuti_list_pengajuan_cuti_nama_text" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:text="Keterangan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/colorTextIcon"
            android:paddingLeft="8dp"
            android:background="@drawable/round_title"
            android:id="@+id/detail_cuti_textView26" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/detail_cuti_list_pengajuan_cuti_tanggal_akhir_cuti_text"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:paddingLeft="8dp"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:id="@+id/detail_cuti_list_pengajuan_cuti_keterangan_text" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/detail_cuti_textView26"/>
    <ImageView
            android:layout_width="46dp"
            android:layout_height="46dp" app:srcCompat="@drawable/ic_account_24dp"
            android:id="@+id/detail_cuti_list_pengajuan_cuti_ttd_pegawai" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            tools:ignore="VectorDrawableCompat" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/detail_cuti_list_pengajuan_cuti_keterangan_text"/>
    <TextView
            android:text="TTD Pegawai"
            android:textStyle="bold"
            android:layout_width="0dp"
            android:textAlignment="textEnd"
            android:layout_height="wrap_content"
            android:id="@+id/detail_cuti_list_pengajuan_cuti_ttd_pegawai_text"
            app:layout_constraintEnd_toStartOf="@+id/detail_cuti_list_pengajuan_cuti_ttd_pegawai"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/detail_cuti_list_pengajuan_cuti_ttd_pegawai"
            app:layout_constraintTop_toTopOf="@+id/detail_cuti_list_pengajuan_cuti_ttd_pegawai"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="46dp"
            android:layout_height="46dp" app:srcCompat="@drawable/ic_account_24dp"
            android:id="@+id/detail_cuti_list_pengajuan_cuti_ttd_admin" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/detail_cuti_list_pengajuan_cuti_ttd_pegawai" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" tools:ignore="VectorDrawableCompat"/>
    <TextView
            android:text="TTD Admin"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textAlignment="textEnd"
            android:id="@+id/detail_cuti_list_pengajuan_cuti_ttd_admin_text"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="@+id/detail_cuti_list_pengajuan_cuti_ttd_admin"
            app:layout_constraintBottom_toBottomOf="@+id/detail_cuti_list_pengajuan_cuti_ttd_admin"
            app:layout_constraintEnd_toStartOf="@+id/detail_cuti_list_pengajuan_cuti_ttd_admin"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>
    <Button
            android:text="Cetak"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/detail_cuti_cetak_btn" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>