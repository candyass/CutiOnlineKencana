<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        app:cardElevation="8dp"
        app:cardBackgroundColor="@android:color/white">

<androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="?android:selectableItemBackground">

    <TextView
            android:text="TextView"
            android:textColor="@color/colorPrimary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/ic_tanggal_24dp"
            android:layout_marginEnd="8dp"
            android:id="@+id/list_pengajuan_cuti_tanggal_cuti_text" app:layout_constraintEnd_toEndOf="parent"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/list_pengajuan_cuti_nama_text"/>
    <TextView
            android:text="Dari Tanggal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView22"
            app:layout_constraintBaseline_toBaselineOf="@+id/list_pengajuan_cuti_tanggal_cuti_text"
            app:layout_constraintEnd_toStartOf="@+id/list_pengajuan_cuti_tanggal_cuti_text" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Menu"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:drawableLeft="@drawable/ic_tanggal_24dp"
            android:textColor="@color/colorPrimary"
            android:layout_height="wrap_content"
            android:id="@+id/list_pengajuan_cuti_tanggal_akhir_cuti_text" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/list_pengajuan_cuti_tanggal_cuti_text" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:text="Sampai Tanggal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView24"
            app:layout_constraintBaseline_toBaselineOf="@+id/list_pengajuan_cuti_tanggal_akhir_cuti_text"
            app:layout_constraintEnd_toStartOf="@+id/list_pengajuan_cuti_tanggal_akhir_cuti_text" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Menu"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="8dp"
            android:textColor="@color/colorTextIcon"
            android:background="@drawable/round_title"
            android:id="@+id/list_pengajuan_cuti_nama_text" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:text="Keterangan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/colorTextIcon"
            android:paddingLeft="8dp"
            android:background="@drawable/round_title"
            android:id="@+id/textView26" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/list_pengajuan_cuti_tanggal_akhir_cuti_text"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:paddingLeft="8dp"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:id="@+id/list_pengajuan_cuti_keterangan_text" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView26"/>
    <ImageView
            android:layout_width="46dp"
            android:layout_height="46dp" app:srcCompat="@drawable/ic_account_24dp"
            android:id="@+id/list_pengajuan_cuti_ttd_pegawai" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/list_pengajuan_cuti_status_text"/>
    <TextView
            android:text="TTD Pegawai"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/list_pengajuan_cuti_ttd_pegawai_text"
            app:layout_constraintEnd_toStartOf="@+id/list_pengajuan_cuti_ttd_pegawai"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/list_pengajuan_cuti_ttd_pegawai"
            app:layout_constraintTop_toTopOf="@+id/list_pengajuan_cuti_ttd_pegawai"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:textAlignment="textEnd"
            android:textColor="@color/colorPrimaryDark"
            android:layout_height="wrap_content"
            android:id="@+id/list_pengajuan_cuti_status_text" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/list_pengajuan_cuti_keterangan_text"
            android:textAppearance="@style/TextAppearance.AppCompat.Tooltip"/>
    <ImageView
            android:layout_width="46dp"
            android:layout_height="46dp" app:srcCompat="@drawable/ic_account_24dp"
            android:id="@+id/list_pengajuan_cuti_ttd_admin" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/list_pengajuan_cuti_ttd_pegawai" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"/>
    <TextView
            android:text="TTD Admin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/list_pengajuan_cuti_ttd_admin_text"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="@+id/list_pengajuan_cuti_ttd_admin"
            app:layout_constraintBottom_toBottomOf="@+id/list_pengajuan_cuti_ttd_admin"
            app:layout_constraintEnd_toStartOf="@+id/list_pengajuan_cuti_ttd_admin" android:layout_marginEnd="8dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>